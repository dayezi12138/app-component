    android:id="@+id/card_v"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/widget_margin_tiny"
    android:background="@drawable/shape_edit"
    android:foreground="?android:attr/selectableItemBackground"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/widget_margin_tiny"
        android:orientation="vertical">

        <TextView
            android:id="@+id/order_num_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center" />

        <TextView
            android:id="@+id/goods_name_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/real_num_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/widget_padding_tiny"
                android:paddingRight="@dimen/widget_padding_tiny"
                android:text="目标产量:" />

            <TextView
                android:id="@+id/real_num_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

            <TextView
                android:id="@+id/report_num_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/widget_padding_tiny"
                android:paddingRight="@dimen/widget_padding_tiny"
                android:text="汇报产量:" />

            <TextView
                android:id="@+id/report_num_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/first_time_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/widget_padding_tiny"
                android:paddingRight="@dimen/widget_padding_tiny"
                android:text="汇报时间:" />

            <TextView
                android:id="@+id/first_time_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/time_on_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/widget_padding_tiny"
                android:paddingRight="@dimen/widget_padding_tiny"
                android:text="交接时差:" />

            <TextView
                android:id="@+id/time_on_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
